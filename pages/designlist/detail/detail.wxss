	height: 100%;
	z-index: 9999;
	background: rgba(0,0,0,0.8)
}
.loginbox{
	width: 15rem;
	height: 8rem;
	background: #fff;
	position: absolute;
	top: 0%;
	bottom: 0%;
	left: 0%;
	right: 0%;
	margin:auto;
	border-radius: .5rem;
}
.logintitle{
	width: 70%;
	margin:auto;
	margin-top: 8%;
	text-align: center;
}
.logintitle:before{
	content: '';
	display: inline-block;
	vertical-align: middle;
	height: 100%;
}
.logintext{
	display: inline-block;
	text-align: center;
	white-space: pre-line;
	font-size: 36rpx;
	margin:auto;
	color: #ec6941;
}
.loginbtn{
	position: absolute;
	bottom: 0%;
	left: 0%;
	width: 100%;
	text-align: center;
	font-size: 34rpx;
	color: #fff;
	background: #ec6941;
	padding:3% 0;
	border-radius: 0 0 .5rem .5rem;
	letter-spacing: 5rpx;
	/*font-family: '黑体';*/

}
