/*评论*/
.discuss-box{
	background:#fff;
	margin-top: 2.5%;
	padding-bottom: 4.5%;
}
.discuss-box .discuss-title{
	padding:2% 0;
	overflow: hidden;
}
.discuss-box .discuss-title text{
   display:block;
   width:2.1%;
   height:50rpx;
   background: #fff720;
   vertical-align: middle;
   margin-right: 2%;
   /*margin-top: -1%;*/
   float: left;
}
.discuss-box .discuss-title view{
	font-size: 30rpx;
	overflow: hidden;
	color: #313131;
}
.discuss-box .discuss-input{
	padding:0 3%;
	overflow: hidden;
	border-bottom:2rpx solid #bfbfbf;
	padding-bottom: 2%;
}
.discuss-box .discuss-input input{
	background: #eee;
	width: 65%;
	border-radius: 56rpx;
	float: left;
	margin-top: 0;
	padding-left: 5%;
	height: auto;
	line-height: auto;
}
.discuss-box .discuss-input .send{
	float:right;
	color:#fff;
	background:#ec6941;
	padding:2% 2%;
	width:16%;
	/*margin-left:3%;*/
	text-align:center;
	border-radius:48rpx;
	font-size: 30rpx;
	letter-spacing: 10rpx;
	padding-left: 4%;
}
.discuss-box .discuss-list{
	overflow: hidden;
	padding:0% 3%;
	padding-top: 3.5%;
}
.discuss-box .discuss-list .discusslist-img{
	float: left;
	width: 15%;
	margin-left: 2%;
}
.discuss-box .discuss-list .discusslist-img image{
	border-radius: 100%;
}
.discuss-box .discuss-list .discusslist-right{
	float: right;
	width: 78%;
	overflow: hidden;
}
.discuss-box .discuss-list .discusslist-right .nickname-box{
	overflow: hidden;
}
.discuss-box .discuss-list .discusslist-right .nickname-box .nickname{
	float: left;
}
.discuss-box .discuss-list .discusslist-right .nickname-box .nickname .name{
	font-size: 28rpx;
	color: #626262;
}
.discuss-box .discuss-list .discusslist-right .nickname-box .nickname .date{
	font-size: 24rpx;
	color: #626262;
}
.discuss-box .discuss-list .discusslist-right .nickname-box .like{
	width: 18%;
	float: right;
}
.discuss-box .discuss-list .discusslist-right .nickname-box .like text{
	font-size: 28rpx;
	margin-right: 11rpx;
	float: right;
}
.discuss-box .discuss-list .discusslist-right .nickname-box .like .like-img{
	width: 35%;
	/*margin-left: -3%;*/
	float: right;
	margin-top: 7%;
}
.discuss-box .discuss-list .discusslist-right .content{
	color: #313131;
	font-size: 26rpx;
	border-bottom:2rpx solid #c8c8c8;
	padding-bottom: 4%;
	margin-top: 1.5%;
}
