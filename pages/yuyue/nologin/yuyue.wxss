
.yuyue::-webkit-input-placeholder {color: #bcbcbc;font-family: "Microsoft YaHei","微软雅黑", Helvetica, Arial, sans-serif;}
.yuyue::input-placeholder {color: #bcbcbc;font-family: "Microsoft YaHei","微软雅黑", Helvetica, Arial, sans-serif;}
.yuyue{background: #fff;}
.citydesign-top{
	width: 100%;
	height: auto;
	background: #fff;
}
.citydesign-top .headimg{
	margin:auto;
	width: 24%;
}
.citydesign-top .headimg image{
	border-radius: 100%;
}
.citydesign-top .name{
	width: 100%;
	margin:2% 0;
	text-align: center;
	margin-bottom: 3%;
}
.citydesign-top .name .username{
	font-size: 36rpx;
	font-weight: 900;
	color: #313131;
}
.citydesign-top .name image{
	width: 38rpx;
	/*margin-top: 1%;*/
	margin:0 1%;
	vertical-align: middle;
	display: inline-block;
}
.citydesign-top .name .sex-img{
	width: 45rpx;
}
.citydesign-top .name .duan-img{
	width: 35rpx;
}
.citydesign-top .name .duanwei{
	font-size: 30rpx;
	color: #313131;
}
.citydesign-top .toptext{
	font-size: 30rpx;
	text-align: center;
	width: 100%;
	color: #313131;
}
/*.yuyue .pic1{width: 750rpx;height: 693rpx;}*/
.yuyue .pic2{display: block;width: 60%;margin: 6% auto;font-size:32rpx;padding:2% 0;font-weight: 400; letter-spacing: 5rpx; border-radius:40rpx;background: #ec6941;color: #fff;text-align: center;animation: spin 1s ease-in-out infinite alternate;}
@-webkit-keyframes spin {
    0% {-webkit-transform: scale(1);}
    50% {-webkit-transform: scale(.9);}
    100% {-webkit-transform: scale(1);}
}
.yuyue .pic3{z-index: 999;display: block;position: fixed;bottom: 3.6rem;background: rgba(0, 0, 0, 0);}
.yuyue-btn{width:62%;height:77rpx;position:absolute;top:0;left:18%;right:0;margin:0 auto;}
.yuyue input{width: 80%;margin: 0 auto;padding: 3% 0;font-size: 30rpx;margin-left: 2.5rem;border: none;background: #eeeeee;}
.psection{color: #c6c6c6;}
.userName{width: 83%;margin: 0 auto;border: 1px solid #ec6941;margin-bottom:2%;background: #eeeeee url(http://img1.homekoocdn.com/html/weixin/hyuser_sys/tenThousand_miniprogram/images/icon-head.png) 0.5rem center/1.5rem auto no-repeat;border-radius:10rpx;}
.userPhone{width: 83%;margin: 0 auto;border: 1px solid #ec6941;background: #eeeeee url(http://img1.homekoocdn.com/html/weixin/hyuser_sys/tenThousand_miniprogram/images/icon-tel.png) 0.5rem center/1.5rem auto no-repeat;border-radius:10rpx;}
.gif {
  background: url(http://img1.homekoocdn.com/html/m_homekoo/miniprogram/images/text.gif) 1.74rem center/1px 0.8rem no-repeat, url(http://img1.homekoocdn.com/html/weixin/hyuser_sys/tenThousand_miniprogram/images/icon-tel.png) 0.5rem center/1.5rem auto no-repeat,#fff!important;
}
.form{width: 100%;margin: 3% auto 0;background: #fff;padding-bottom: 19%;}
.form text{display: block;font-size: 24rpx;text-align: center;color: #626262;margin-top:12rpx;}
/*蒙版*/
.yuyueMb{width: 100%;height:100%;background: rgba(0,0,0,0.9);position: fixed;top: 0;left: 0;right: 0;margin: 0 auto;z-index: 9;overflow: auto;display: none;z-index: 999;}
.yuyueMb-img{
	width: 80%;
	position: absolute;
	top: 0%;
	height: 14rem;
	bottom: 0%;
	left: 0%;
	right: 0%;
	margin:auto;
}
.yuyueMb-img image{border-radius: .8rem;}
page{background: #fff;}
.yuyue-close{
	position: absolute;
	top: 5%;
	right: 5%;
	width: 8%;
}
