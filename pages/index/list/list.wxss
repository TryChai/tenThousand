page{background: #eeeeee;}
swiper{
  height:350rpx;
}
swiper-item{
  width: 100%;
  height: 100%;
  display: block;
}
swiper-item image{
  width: 100%;
  height: 100%;
}
.nav {
  padding:3% 3%;
  background: #fff;
  overflow: hidden;
}
.nav view{
  float: left;
  width:20%;
  margin:0 2.5%;
}
.nav view image{
  width:100%;
}
.view-box{
  margin-top: 2%;
  background: #fff;
  padding:3% 0;
}
.view-title{
  font-size: 32rpx;
}
.view-title text{
  display: inline-block;
  width:2.1%;
  height:50rpx;
  background: #fff720;
  vertical-align: middle;
  margin-right: 2%;
  /*margin-top: -1%;*/
}
.view-img{
  padding:2% 5% 0;
}
.view-img image{
  width:100%;
  padding-bottom: 1%;
}
.view-content{
  font-size: 30rpx;
  padding:3% 0%;
}
.bottom{
  width:100%;
  height: 123rpx;
}
.loading-box{
  padding:3% 0 ;
  text-align: center;
  overflow: hidden;
}
.loading-box .loading-massage{
  overflow: hidden;
  width:100%;
}
.loading-box .loading-massage .loading-img{
  width:6%;
  float: left;
  margin-left: 40%;
  margin-right: 2%;
}
.loading-box .loading-massage .loading-img image{
  width:100%;
}
.loading-box .loading-massage .loading-text{
  text-align: center;
  font-size: 30rpx;
  float: left;
  color: #c2c2c2;
}
.loading-box .nomore{
  display: block;
  font-size: 34rpx;
  clear: both;
}