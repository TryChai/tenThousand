page{background: #fff;}
.nav {
  padding:3% 3%;
  background: #fff;
  overflow: hidden;
}
.nav view{
  float: left;
  width:20%;
  margin:0 2.5%;
}
.nav view image{
  width:100%;
}
.databox{
  border-top:30rpx solid #eee;
}
.databox .databox-top{
  overflow: hidden;
  padding:3% 5%;
}
.databox .databox-top .databox-topimg{
  width: 18%;
  float: left;
}
.databox .databox-top .databox-topimg image{
  border-radius: 100%;
}
.databox .databox-top .databox-toptext{
  float: left;
  margin-left:4%;
  overflow: hidden;
  width: 40%;
  margin-top: 1%;
}
.databox .databox-top .databox-loginout{
  float: right;
  font-size:24rpx;
  color: #e9611d;
  text-align: right;
}
.databox .databox-top .databox-toptext .username{
  overflow: hidden;
  margin-bottom: 1%;
}
.databox .databox-top .databox-toptext .username text{
  float: left;
  margin-right: 3%;
  font-size: 36rpx;
  font-weight: 900;
}
.databox .databox-top .databox-toptext  .username .sexmark{
  float: left;
  width: 16%;
}
.databox .databox-top .databox-toptext .zhicheng{
  overflow: hidden;
}
.databox .databox-top .databox-toptext .zhicheng text{
  float: left;
  margin-right: 3%;
  font-size: 32rpx;
  font-weight: 900;
}
.databox .databox-top .databox-toptext  .zhicheng .sexmark{
  float: left;
  width: 14%;
}
.databox  .liangchi{
  background:#fff720;
  width: 85%;
  margin:auto;
  padding:2% 0;
  border-radius:100rpx;
  overflow: hidden;
}
.databox  .liangchi .liangchi-left{
  width: 49%;
  float: left;
  border-right: 3rpx solid #7d7d7d; 
  text-align: center;
}
.databox  .liangchi .liangchi-left .number{
  font-size: 70rpx;
  color: #ea6948;
}
.databox  .liangchi .liangchi-left .text{
  font-size: 30rpx;
  color: #626262;
  font-weight: 900;
}
.databox  .liangchi .liangchi-right{
  width: 50%;
  float: left;
  text-align: center;
}
.databox  .liangchi .liangchi-right .number{
  font-size: 70rpx;
  color: #313131;
}
.databox  .liangchi .liangchi-right .text{
  font-size: 30rpx;
  color: #626262;
  font-weight: 900;
}
.databox  .datebox{
  width: 92%;
  margin:5% auto;
  overflow: hidden;
}
.databox  .datebox .date-bg{
  overflow: hidden;
  float: left;
  width: 48%;
  border-radius: 60rpx;
}
.databox  .datebox .date-bg:last-child{
  float: right;
}
.databox  .datebox .picker{
  float: left;
  width: 100%;
  font-size: 28rpx;
  padding-left: 5%;
  border-radius: 60rpx;
  /*padding:2% 0;*/
  margin-top: 0;
  background: #eeeeee url(http://img1.homekoocdn.com/html/weixin/hyuser_sys/tenThousand_miniprogram/images/design-date.png) 88% center/.5rem no-repeat;
}
.databox  .datebox .picker:nth-child(2){
  float: right;
}
/*.databox  .datebox .date-bg .picker{
  margin-top: 0%;
}*/
/*.databox  .datebox view:nth-child(1){
  margin-right: 3%;
}*/
.databox  .designdata{
  padding:0 5%;
}
.databox  .designdata .yuedu{
  overflow: hidden;
  border-bottom: 2rpx solid #bfbfbf;
}
.databox  .designdata .yuedu view{
  float: left;
  padding:2% 0;
  font-size: 28rpx;
  color: #3a3a3a;
}
.databox  .designdata .yuedu view:last-child{
  float: right;
}
.databox  .designdata .baodata{
  display: flex;
}
.databox  .designdata .baodata view{
  padding:2% 0;
  width: 33.3%;
  font-size: 28rpx;
  color: #3a3a3a;
}
.databox  .designdata .baodata view:nth-child(2){
  text-align: center;
}
.databox  .designdata .baodata view:nth-child(3){
  text-align: right;
}
.loading-box{
  padding:3% 0 ;
  text-align: center;
  overflow: hidden;
}
.loading-box .loading-massage{
  overflow: hidden;
  width:100%;
}
.loading-box .loading-massage .loading-img{
  width:6%;
  float: left;
  margin-left: 40%;
  margin-right: 2%;
}
.loading-box .loading-massage .loading-img image{
  width:100%;
}
.loading-box .loading-massage .loading-text{
  text-align: center;
  font-size: 30rpx;
  float: left;
  color: #c2c2c2;
}
.loading-box .nomore{
  display: block;
  font-size: 34rpx;
  clear: both;
}