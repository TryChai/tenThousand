/**app.wxss**/
image{display: block;}
image{width: 100%;}
input,.picker{border:solid 1px #eee;background: #fff;padding:0.3rem;margin-top:0.5rem;height:1.4rem;line-height: 1.4rem;}
modal text{padding:6px 0 0 6px;}
.clearfix:after{content: '';clear: both;display: block;height:1px;}
page{background:#fafafa;font-size:14px;color:#666;line-height: 1.5;position: relative;min-height:100%;box-sizing:border-box;font-family:PingFangSC-Regular,Tahoma,Microsoft Yahei;}
page scroll-view .page-list,page .footer{padding-bottom: 3.6rem;}
.border-bottom{width: 100%;height: 1px;border-bottom: 1px solid #bdbdbd;position: fixed;bottom: 0;}
/*宽度*/
.w{width:680rpx;margin:0 auto;}
.full{width:100%;}

/*定位*/
.pr{position: relative;}
.pa{position: absolute;}

/*文字超出省略*/
.ellipsis {
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    display: block;
}

/*对齐*/
.tl{text-align: left;}
.tc{text-align: center;}
.tr{text-align: right;}

/*间距*/
.mt5{margin-top: 5rpx;}
.mt10{margin-top: 10rpx;}
.mt15{margin-top: 15rpx;}
.mt20{margin-top: 20rpx;}
.mt25{margin-top: 25rpx;}
.mt30{margin-top: 30rpx;}
.mt35{margin-top: 35rpx;}
.mt40{margin-top: 40rpx;}

/*背景*/
.bg_f{background: #fff}

/*边框*/
.border_t{border-top:solid 1px #eee;}
.border_b{border-bottom: solid 1px #eee;}

/*浮动*/
.fl{float:left;}
.fr{float:right;}

/*弹性盒模型*/
.box {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
}
.flex1 {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    -ms-flex: 1;
    box-flex: 1;
    flex: 1;
    overflow: hidden;
    display: block;
}

/*footer*/
.footer_box{height:60px;width:750rpx;line-height: 60px;margin-top:20rpx;}
.footer{font-size: 12px;bottom:0;width:750rpx;}

/*无结果提示*/
.null{margin-top: 100rpx}
.null .txt1{color:#000;font-size:38rpx;margin-top: 20rpx;}
.null .txt2{font-size:30rpx;}
/*loading图*/
/*下拉加载更多*/
.h40 {
  width: 100%;
  height: 50px;
/*  padding-bottom: 120px;*/
}
.MoreTex {
  text-align: center;
  color: #b0aeb0;
  padding-top: .7rem;
  font-size: 36rpx;
  line-height: 1.5rem;
  /*display: none;*/
}
.loading {
  bottom: 0;
  display: none; 
  width: 100%;
  color: #b0aeb0;
  text-align: center;
  padding-top: .7rem;
  font-size: 36rpx;
}
.icon {
  display: inline-block;
  width: 46rpx;
  height: 46rpx;
  background: url(http://img1.homekoocdn.com/html/weixin/hyuser_sys/everyday_new_miniprogram/images/i5.png) no-repeat;
  background-size: 100%;
  vertical-align: middle;
  margin: -2.5px 7.5px 0;
  -webkit-animation: shou 2s linear infinite;
  animation: shou 2s linear infinite;
  transform: rotate(0);
}
@-webkit-keyframes shou {
    0% {
        -webkit-transform: rotate(0deg);
    }
    100% {
        -webkit-transform: rotate(360deg);
    }
}
/*滚动设置*/
.scroll_view{/*position: absolute;*//*height: 100%;*//*padding-bottom:3.6rem;*/overflow:hidden;}
.fdong{width: 72rpx;height: 72rpx;position: fixed;right: 32rpx;bottom: 4rem;}
/*底部点赞 收藏*/
.footer-icon{text-align: right;font-size: 26rpx;margin: 70rpx 0 30rpx;}
.footer-icon view,.footer-icon text{display: inline-block;vertical-align: middle;}
.read-num .read-icon{width: 30rpx;height: 20rpx;background: url(http://img1.homekoocdn.com/html/weixin/hyuser_sys/everyday_new_miniprogram/images/main_icon.png) center -320rpx no-repeat;background-size: 39rpx 425rpx;}
.cang-num .cang-icon{width: 30rpx;height: 32rpx;background: url(http://img1.homekoocdn.com/html/weixin/hyuser_sys/everyday_new_miniprogram/images/main_icon.png) center -44rpx no-repeat;background-size: 39rpx 425rpx;}
.zhuan-num .zhuan-icon{width: 30rpx;height: 30rpx;background: url(http://img1.homekoocdn.com/html/weixin/hyuser_sys/everyday_new_miniprogram/images/main_icon.png) center -270rpx no-repeat;background-size: 39rpx 425rpx;}
.read-num,.cang-num,.zhuan-num{margin-left: 10rpx;}
.footer{padding: 0 20rpx;box-sizing:border-box;}
.footer-world{font-size: 30rpx;color: #888888;text-align: center;margin-bottom: 60rpx;}
/*头像*/
.tx-box view:nth-child(8n+0){margin-right: 0;}
.tx{width: 80rpx;height: 80rpx;display: inline-block;vertical-align: middle;margin: 0 10rpx 10rpx 0;}
.tx image{border-radius: 100%;-webkit-border-radius: 100%;width: 80rpx;height: 80rpx;}
/*点赞icon*/
.zan-box {text-align: center;margin: 32rpx 0 52rpx 0;}
.zan-box text,.zan-box .zan-box-icon,.zan-img{display: inline-block;vertical-align: middle;}
.zan-box-icon{width: 70rpx;height: 70rpx;line-height: 70rpx;border: 2px solid #888888;border-radius: 100%;text-align: center;}
.zan-box text{font-size: 30rpx;line-height:0;margin-left: 10rpx;}/*22rpx -104rpx*/
.zan-img{width: 30rpx;height: 28rpx;-webkit-border-radius: 100%;background: url(http://img1.homekoocdn.com/html/weixin/hyuser_sys/everyday_new_miniprogram/images/main_icon.png) -1rpx -3rpx no-repeat;background-size: 39rpx 425rpx;}
.click_cur .zan-box-icon{border:2px solid #0ed0c5;  -webkit-animation: Animate .8s 0s ease-in-out forwards;
  animation: Animate .8s 0s ease-in-out forwards;}
.click_cur text{color: #0ed0c5}
.click_cur .cang-icon{background:url(http://img1.homekoocdn.com/html/weixin/hyuser_sys/everyday_new_miniprogram/images/main_icon.png) center -168rpx no-repeat;background-size:39rpx 425rpx;-webkit-animation: Animate .8s 0s ease-in-out forwards;
  animation: Animate .8s 0s ease-in-out forwards;}
.click_cur .zan-img{background:url(http://img1.homekoocdn.com/html/weixin/hyuser_sys/everyday_new_miniprogram/images/main_icon.png) -1rpx -126rpx no-repeat;background-size:39rpx 425rpx;}
.guess-like-icon text,.guess-like-icon image{display: inline-block;vertical-align: middle;}
.guess-like-icon{height: 76rpx;line-height: 76rpx;background: #0ed0c5;color: #fff;padding: 0 20rpx;box-sizing:border-box;}
.guess-like-icon image{width: 44rpx;height: 38rpx;}
.guess-like-icon text{font-size: 38rpx;margin: -2rpx 0 0 16rpx;}
.guess-like-li{  border-bottom: 1px solid #efefef;padding: 14px 0;font-size: 32rpx;overflow: hidden;text-overflow: ellipsis;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;}
.guess-like-li-p{  width: 90%;vertical-align: middle;overflow: hidden;text-overflow: ellipsis;display: -webkit-inline-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;}
.arrow-right{  width: 10%;display: inline-block;text-align: right;vertical-align: middle;}
.arrow-right-img{  display: inline-block;vertical-align: middle;width: 16rpx;height: 29rpx;background: url(http://img1.homekoocdn.com/html/weixin/hyuser_sys/everyday_new_miniprogram/images/arrow-r.png) center center no-repeat;background-size: 100% 100%;}
@keyframes Animate{
    0% {-webkit-transform: scale(.8);}
    50% {-webkit-transform: scale(1);}
    100% {-webkit-transform: scale(.8);}
}
.read-num-a{font-size: 24rpx;margin:100rpx 0 20rpx 0;text-align: right;}
.read-num-b{margin-right: 12rpx;}
